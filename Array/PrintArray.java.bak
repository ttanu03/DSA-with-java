import java.util.Scanner;
class PrintArray 
{
	public static void zeroCount( int a[])
	{
		int zeroCount=0;
		int oneCount=0;
		for(int i=0;i<a.length;i++)
		{
			if(a[i]==0)
			{
				zeroCount++;
			}
			if( a[i]==1)
			{
				oneCount++;
			}
		}
		System.out.println(" The number of zero in a array " + zeroCount);
		System.out.println(" The number of one  in an array " + oneCount);
	}

	public static void main(String[] args) 
	{
		Scanner sc= new Scanner(System.in);
		int  a [] = new int[]{2,3,4,5,0,0,0,1,0,1,0,1,1,1,0};
	    zeroCount(a);
		
	}
		
	
}
