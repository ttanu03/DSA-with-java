import java.util.Scanner;
class LearningArray  
{
	public static void main(String[] args) 
	{
		Scanner input=new Scanner(System.in);
		int a []=new int[10];
		System.out.println("Enter the element of the array");
        for(int i=0;i<10;i++)
		{
		a[i]= input.nextInt();
		}

		for(int i=0;i<10;i++)
		{
			System.out.println(2*a[i]);
		}
	
	}
}
